    "faq_KdyWzh": {
      "type": "faq",
      "blocks": {
        "frequently_asked_question_Q3wVRx": {
          "type": "frequently_asked_question",
          "settings": {
            "icon": "faq",
            "another_icon": "",
            "custom_icon": "",
            "heading": "What payment methods do you accept?",
            "row_content": "<p>We accept various payment methods, including credit/debit cards (Visa, Mastercard, American Express), PayPal, and in some locations, cash on delivery.</p>"
          }
        },
        "frequently_asked_question_GihRtG": {
          "type": "frequently_asked_question",
          "settings": {
            "icon": "faq",
            "another_icon": "",
            "custom_icon": "",
            "heading": "Do you offer international shipping?",
            "row_content": "<p>Yes, we offer international shipping to many countries. Shipping costs and delivery times may vary depending on your location. Please refer to our shipping policy or contact our customer service team for more information.</p>"
          }
        },
        "frequently_asked_question_KVdWRq": {
          "type": "frequently_asked_question",
          "settings": {
            "icon": "faq",
            "another_icon": "",
            "custom_icon": "",
            "heading": "How can I track my order?",
            "row_content": "<p>Once your order has been shipped, you will receive a tracking number via email or SMS, depending on the contact information provided at checkout. You can use this tracking number to monitor the status of your delivery.</p>"
          }
        }
      },
